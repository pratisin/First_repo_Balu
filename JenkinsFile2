pipeline {
   agent any
   tools {
        maven 'Maven'

    }
   stages {
        stage('Build@!#$ %') {
            steps {
               snDevOpsStep ()
                echo "build" 
            }
        }
      
       

      
       stage('Deploy @!#$ %') {
             steps{
                  snDevOpsStep ()
                  snDevOpsChange()
                  echo 'deployment started'      
              }
      }
      
  }
}
